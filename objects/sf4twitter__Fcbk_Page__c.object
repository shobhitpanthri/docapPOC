    <webLinks>
        <fullName>sf4twitter__Get_Page_Info</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Get Page Info</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>var results = (new RegExp(&quot;[\\?&amp;]isdtp=([^&amp;#]*)&quot;)).exec(window.location.href);
if ( results == null ){
window.location = &apos;/apex/sf4twitter__FcbkUpdatePageInfo?accId={!sf4twitter__Fcbk_Page__c.Id}&amp;records={!sf4twitter__Fcbk_Page__c.Id}&amp;retURL=&apos; + encodeURIComponent(window.location);
}else{
window.location = &apos;/apex/sf4twitter__FcbkUpdatePageInfo?isdtp=&apos;+results[1]+&apos;&amp;accId={!sf4twitter__Fcbk_Page__c.Id}&amp;records={!sf4twitter__Fcbk_Page__c.Id}&amp;retURL=&apos; + encodeURIComponent(window.location);
}</url>
    </webLinks>
    <webLinks>
        <fullName>sf4twitter__Revoke_Facebook_Access</fullName>
        <availability>online</availability>
        <description>if Validated is checked, see the Revoke button.</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Revoke Facebook Access</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/13.0/connection.js&quot;)} 

if({!sf4twitter__Fcbk_Page__c.sf4twitter__Valid__c}){
    var agree=confirm(&quot;{!$Label.sf4twitter__REVOKE_FACEBOOK_PAGE_ACCESS_CONFIRM}&quot;);

   if (agree){

       window.location = &quot;/apex/sf4twitter__FcbkOAuth?revoke=1&amp;accId={!sf4twitter__Fcbk_Page__c.Id}&amp;retURL=&quot; + encodeURIComponent(window.location) ;

   }
}else{
   alert(&apos;{!JSENCODE($Label.sf4twitter__REVOKE_PAGE_NOT_VALIDATED)}&apos;);
}</url>
    </webLinks>
    <webLinks>
        <fullName>sf4twitter__Schedule_Facebook_Pages_Searches</fullName>
        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Schedule Facebook Pages Searches</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>false</requireRowSelection>
        <url>var results = (new RegExp(&quot;[\\?&amp;]isdtp=([^&amp;#]*)&quot;)).exec(window.location.href);
if ( results == null ){
    window.location.href = &apos;/apex/sf4twitter__TwitterScheduler&apos;;
}else{
    window.location.href = &apos;/apex/sf4twitter__TwitterScheduler?isdtp=&apos;+results[1];
}</url>
    </webLinks>
